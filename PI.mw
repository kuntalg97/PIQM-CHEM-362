<Input><Text-field prompt="&gt; " style="Maple Input" size="10" family="P052" layout="Normal"><Font size="10" style="Text" family="P052" italic="true">Initialising parameters</Font><Font size="10" family="P052">
n := 200:
rmin := 1.4:
rmax := 1.7:
dr := (rmax-rmin)/(n-1):
